{
    "name": "advance-username-generator",
    "version": "2.0.2",
    "description": "advance-username-generator is a simple Node.js package that provides a utility function to generate unique usernames. It can be used in applications where a random or unique username is required, such as user registration systems, chat applications, or any scenario where unique identifiers are needed.",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc --declaration",
        "start": "node dist/index.js",
        "test": "jest --watchAll=false"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/liu-purnomo/advance-username-generator.git"
    },
    "keywords": [
        "username",
        "generator",
        "unique"
    ],
    "author": "liupurnomo.com",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/liu-purnomo/advance-username-generator/issues"
    },
    "homepage": "https://github.com/liu-purnomo/advance-username-generator#readme",
    "devDependencies": {
        "@types/jest": "^29.5.12",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.2",
        "typescript": "^5.3.3"
    },
    "jest": {
        "preset": "ts-jest",
        "testEnvironment": "node",
        "testMatch": [
            "**/__tests__/**/*.[jt]s?(x)",
            "**/?(*.)+(spec|test).[tj]s?(x)"
        ]
    }
}
